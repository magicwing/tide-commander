{
  "name": "tide-commander",
  "version": "0.2.0",
  "description": "RTS/MOBA-style Claude Code agents commander",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:client\" \"bun run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/packages/server/index.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "setup": "tsx src/packages/server/setup.ts",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "chokidar": "^4.0.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "fuse.js": "^7.1.0",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.170.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.170.0",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.1.0",
    "sass": "^1.97.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.17"
  }
}
