{
  "name": "tide-commander",
  "version": "0.64.0",
  "description": "Visual multi-agent orchestrator and manager for Claude Code with 3D/2D interface",
  "type": "module",
  "bin": {
    "tide-commander": "./dist/src/packages/server/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "PORT=5174 LISTEN_ALL_INTERFACES=1 concurrently \"bun run dev:client\" \"bun run dev:server\"",
    "dev:5174": "PORT=5174 LISTEN_ALL_INTERFACES=1 concurrently \"bun run dev:client\" \"bun run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/packages/server/index.ts",
    "dev:landing": "vite --open /src/packages/landing/index.html",
    "build": "npm run build:types && vite build && npm run build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "setup": "tsx src/packages/server/setup.ts",
    "lint": "eslint src/",
    "build:types": "tsc --noEmit",
    "lint:types": "tsc --noEmit",
    "test": "vitest run",
    "test:ci": "vitest run --exclude src/packages/client/hooks/__tests__/useSnapshots.test.ts --exclude src/packages/client/hooks/__tests__/useKeyboardShortcuts.test.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/oracledb": "^6.10.1",
    "archiver": "^7.0.1",
    "chokidar": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fuse.js": "^7.1.0",
    "i18next": "^25.8.7",
    "i18next-browser-languagedetector": "^8.2.1",
    "i18next-http-backend": "^3.0.2",
    "mysql2": "^3.16.2",
    "oracledb": "^6.10.0",
    "pg": "^8.17.2",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.170.0",
    "unzip-stream": "^0.3.4",
    "ws": "^8.18.0"
  },
  "overrides": {
    "tar": "7.5.7"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.170.0",
    "@types/unzip-stream": "^0.3.4",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.2",
    "sass": "^1.97.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.17"
  }
}
